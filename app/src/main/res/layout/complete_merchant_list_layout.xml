<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="10dp">


            <TextView
                android:id="@+id/mercode_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MERCHANT CODE"
                android:textColor="#000000"
                android:textSize="12dp"/>

            <TextView
                android:id="@+id/merchantcode_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="user_merchantcode"
                android:layout_marginLeft="40dp"
                android:textColor="#000000"
                android:textSize="20dp"
                />

            <TextView
                android:id="@+id/mercat_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MERCHANT CATEGORY"
                android:textColor="#000000"
                android:textSize="12dp"/>

            <TextView
                android:id="@+id/merchantcategory_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="user_merchantcategory"
                android:layout_marginLeft="40dp"
                android:textColor="#000000"
                android:textSize="20dp"

                />


            <TextView
                android:id="@+id/mername_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MERCHANT NAME"
                android:textColor="#000000"
                android:textSize="12dp"/>

            <TextView
                android:id="@+id/merchantname_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:text="user_merchantname"
                android:textColor="#000000"
                android:textSize="20dp"
                />

    <TextView
        android:id="@+id/ecashbal_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E_CASH BALANCE:"
        android:textColor="#000000"
        android:textSize="12dp"/>

    <TextView
        android:id="@+id/ecashbalance_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="user_ecashbalance"
        android:textColor="#000000"
        android:layout_marginLeft="40dp"
        android:textSize="20dp"
        />


    <LinearLayout
        android:id="@+id/id10"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/Edit_textview"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="EditMerchant"
        android:textAlignment="center"
        android:layout_marginLeft="20dp"
        android:textStyle="bold"
        android:background="@color/colorButton"
        android:textColor="#FFFFFF"/>
        <TextView
            android:id="@+id/viewterminal_textview"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="ViewTerminals"
            android:layout_marginLeft="120dp"
            android:textStyle="bold"
            android:textAlignment="center"
            android:background="@color/colorButton"
            android:textColor="#FFFFFF"/>

        </LinearLayout>


    <View
        android:background="#000000"
        android:id="@+id/id7"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>

</LinearLayout>