<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/id9"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="20dp"
    android:background="#ddd"
    android:layout_marginRight="10dp"
    android:layout_marginLeft="10dp">
<android.support.v7.widget.CardView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    >
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">
        <TextView
            android:id="@+id/mercode_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Merchant Code"
            android:textSize="12dp"
            android:textColor="#000000"
           />

        <TextView
            android:id="@+id/merchantcode_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="user_merchantcode"
            android:textColor="#000000"
            android:layout_marginLeft="40dp"
            android:textSize="20dp"
           />


        <TextView
            android:id="@+id/mercat_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Merchant Category"
            android:textColor="#000000"
            android:textSize="12dp"/>

        <TextView
            android:id="@+id/merchantcategory_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="user_merchantcategory"
            android:layout_marginLeft="40dp"
            android:textColor="#000000"
            android:textSize="20dp"
            />

        <TextView
            android:id="@+id/mername_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Merchant Name"
            android:textColor="#000000"
            android:textSize="12dp"/>

        <TextView
            android:id="@+id/merchantname_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="user_merchantname"
            android:layout_marginLeft="40dp"
            android:textColor="#000000"
            android:textSize="20dp"
            />
    <TextView
        android:id="@+id/approve_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_gravity="center"
        android:text="Approve"
        android:background="@color/colorButton"
        android:textAppearance="?android:textAppearanceLarge"
        android:textColor="#FFFFFF"/>
</LinearLayout>
</android.support.v7.widget.CardView>
</LinearLayout>